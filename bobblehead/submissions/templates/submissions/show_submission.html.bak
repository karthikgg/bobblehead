<h1>{{submission.project.title}}</h1>

<h3>Submission link</h3>
{{submission.link}}

<h3>Description</h3>
{{submission.description}}

<h3>List of members</h3>
{% for member in submission.members.all %}
<p>User Name: {{member.email}}</p>
{% endfor %}

<a href="/projects/{{submission.project.id}}">Return</a>
